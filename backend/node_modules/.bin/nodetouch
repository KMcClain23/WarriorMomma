#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/WarriorMomma/backend/node_modules/touch/bin/node_modules:/workspaces/WarriorMomma/backend/node_modules/touch/node_modules:/workspaces/WarriorMomma/backend/node_modules:/workspaces/WarriorMomma/node_modules:/workspaces/node_modules:/node_modules:/workspaces/WarriorMomma/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/WarriorMomma/backend/node_modules/touch/bin/node_modules:/workspaces/WarriorMomma/backend/node_modules/touch/node_modules:/workspaces/WarriorMomma/backend/node_modules:/workspaces/WarriorMomma/node_modules:/workspaces/node_modules:/node_modules:/workspaces/WarriorMomma/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
